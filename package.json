  "dependencies": {
    "@types/node": "^12.12.6",
    "sinusbot-scripting-engine": "^1.0.16"
  },
  "devDependencies": {
    "@types/jest": "^24.0.22",
    "documentation": "^12.1.3",
    "eslint": "^6.6.0",
    "jest": "^24.9.0",
    "sinusbot-test-environment": "git+https://github.com/Multivit4min/Sinusbot-Test-Environment.git"
